datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String

  notes    Note[]
}

model Note {
  id       Int    @id @default(autoincrement())
  titre    String
  contenu  String

  userId   Int
  user     User   @relation(fields: [userId], references: [id], onDelete: Cascade)
}
